<Window x:Class="Data.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Data"
        mc:Ignorable="d" Name="wdnPrincipal"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtEjemplo"  HorizontalAlignment="Left" Height="23" Margin="106,78,0,0" TextWrapping="Wrap" Text="popo" VerticalAlignment="Top" Width="120"/>
        <TextBlock Text="{Binding Path = Text, ElementName=txtEjemplo}" HorizontalAlignment="Left" Margin="106,125,0,0" TextWrapping="Wrap"  VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="106,194,0,0" TextWrapping="Wrap" Text="Ancho" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="197,193,0,0" TextWrapping="Wrap" Text="{Binding Path=Width, ElementName=wdnPrincipal}" VerticalAlignment="Top" Width="120" />
        <TextBlock HorizontalAlignment="Left" Margin="106,231,0,0" TextWrapping="Wrap" Text="Alto" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="197,230,0,0" TextWrapping="Wrap" Text="{Binding Path=Heightn, ElementName=wdnPrincipal}" VerticalAlignment="Top" Width="120"/>




    </Grid>
</Window>
